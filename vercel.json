{
  "rewrites": [
    {
      "source": "/v2/:match*",
      "destination": "https://dash-v2.vercel.app/:match*"
    },
    {
      "source": "/v2",
      "destination": "https://dash-v2.vercel.app/inspections"
    },
    {
      "source": "/:match*",
      "destination": "https://dash-v1.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "https://dash-v1.vercel.app/inspections"
    }
  ]
}
