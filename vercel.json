{
  "rewrites": [
    {
      "source": "/v2/:match*",
      "destination": "https://vercel-routing-example-v2.vercel.app/v2/:match*"
    },
    {
      "source": "/v2",
      "destination": "https://vercel-routing-example-v2.vercel.app/v2/example"
    },
    {
      "source": "/:match*",
      "destination": "https://vercel-routing-example-v1.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "https://vercel-routing-example-v1.vercel.app/example"
    }
  ]
}
